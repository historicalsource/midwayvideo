	@$(MAKE) --no-print-directory -f nflbios.mak clean
	@$(MAKE) --no-print-directory -f nfltest.mak clean
	@$(MAKE) --no-print-directory -f b99bios.mak clean
	@$(MAKE) --no-print-directory -f b99ltest.mak clean
	@$(MAKE) --no-print-directory -f srbios.mak clean
	@$(MAKE) --no-print-directory -f srtest.mak clean
	@$(MAKE) --no-print-directory -f carnbios.mak clean
	@$(MAKE) --no-print-directory -f carntest.mak clean
	@$(MAKE) --no-print-directory -f vegabios.mak clean
	@$(MAKE) --no-print-directory -f vegatest.mak clean

rebuild:
