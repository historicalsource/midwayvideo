	@$(MAKE) --no-print-directory -f nbatest.mak rebuild
	@$(MAKE) --no-print-directory -f nflbios.mak rebuild
	@$(MAKE) --no-print-directory -f nfltest.mak rebuild
	@$(MAKE) --no-print-directory -f b99bios.mak rebuild
	@$(MAKE) --no-print-directory -f b99test.mak rebuild
	@$(MAKE) --no-print-directory -f srbios.mak rebuild
	@$(MAKE) --no-print-directory -f srtest.mak rebuild
	@$(MAKE) --no-print-directory -f carnbios.mak rebuild
	@$(MAKE) --no-print-directory -f carntest.mak rebuild
	@$(MAKE) --no-print-directory -f vegabios.mak rebuild
	@$(MAKE) --no-print-directory -f vegatest.mak rebuild
