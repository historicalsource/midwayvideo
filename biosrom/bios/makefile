	@$(MAKE) --no-print-directory -f nflbios.mak clobber
	@$(MAKE) --no-print-directory -f nfltest.mak clobber
	@$(MAKE) --no-print-directory -f b99bios.mak clobber
	@$(MAKE) --no-print-directory -f b99ltest.mak clobber
	@$(MAKE) --no-print-directory -f srbios.mak clobber
	@$(MAKE) --no-print-directory -f srtest.mak clobber
	@$(MAKE) --no-print-directory -f carnbios.mak clobber
	@$(MAKE) --no-print-directory -f carntest.mak clobber
	@$(MAKE) --no-print-directory -f vegabios.mak clobber
	@$(MAKE) --no-print-directory -f vegatest.mak clobber

